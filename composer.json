{
    "name": "popphp/pop-kettle",
    "description": "A CLI helper application for the Pop PHP Framework.",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "popphp/popcorn": "^4.0.0",
        "popphp/pop-code": "^5.0.0",
        "popphp/pop-console": "^4.1.0",
        "popphp/pop-db": "^6.0.0",
        "popphp/pop-dir": "^4.0.0",
        "vlucas/phpdotenv": "^5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.0",
        "vlucas/phpdotenv": "^5.6.0",
        "ext-sqlite3": "*"
    },
    "autoload" : {
        "psr-4": {
            "Pop\\Kettle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Kettle\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
