{
    "name": "popphp/pop-kettle",
    "description": "A CLI helper application for the Pop PHP Framework.",
    "keywords": [
        "console helper",
        "CLI helper",
        "pop",
        "pop php",
        "php"
    ],
    "homepage": "https://github.com/popphp/pop-kettle",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@noladev.com",
            "homepage": "https://www.popphp.org/"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "popphp/popcorn": "^4.1.1",
        "popphp/pop-code": "^5.0.2",
        "popphp/pop-console": "^4.2.2",
        "popphp/pop-db": "^6.5.11",
        "popphp/pop-dir": "^4.0.0",
        "vlucas/phpdotenv": "^5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.0",
        "ext-sqlite3": "*"
    },
    "autoload" : {
        "psr-4": {
            "Pop\\Kettle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Kettle\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    }
}
